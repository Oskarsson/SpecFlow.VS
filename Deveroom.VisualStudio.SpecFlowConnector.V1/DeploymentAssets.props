      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <IncludeInVSIX>true</IncludeInVSIX>
      <Visible>False</Visible>
      <VSIXSubPath>$(DeploymentV1TargetDir)</VSIXSubPath>
    </Content>
    <Content Include="$(DeploymentV1OutputDir)\Deveroom.VisualStudio.SpecFlowConnector.Models.pdb">
