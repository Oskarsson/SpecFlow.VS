    <Content Include="$(DeploymentV1OutputDir)\Newtonsoft.Json.dll">
      <Link>$(DeploymentV1TargetDir)\Newtonsoft.Json.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <IncludeInVSIX>true</IncludeInVSIX>
      <Visible>False</Visible>
      <VSIXSubPath>$(DeploymentV1TargetDir)</VSIXSubPath>
