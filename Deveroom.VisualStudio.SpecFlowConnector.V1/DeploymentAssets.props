<?xml version="1.0" encoding="utf-8" ?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <DeploymentV1OutputDir>$(MSBuildThisFileDirectory)bin\$(Configuration)\net452\win7-x86</DeploymentV1OutputDir>
    <DeploymentV1TargetDir>Connectors\V1</DeploymentV1TargetDir>
  </PropertyGroup>
  <ItemGroup>
